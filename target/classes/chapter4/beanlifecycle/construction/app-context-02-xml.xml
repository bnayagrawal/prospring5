<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
       default-lazy-init="true">

    <!-- bean definitions here -->

    <bean id="singerOne" class="dev.bnayagrawal.prospring5.chapter4.beanlifecycle.construction.SingerWithInterface"
          p:name="John Mayer" p:age="39"/>

    <bean id="singerTwo" class="dev.bnayagrawal.prospring5.chapter4.beanlifecycle.construction.SingerWithInterface"
          p:age="72"/>
    <bean id="singerThree" class="dev.bnayagrawal.prospring5.chapter4.beanlifecycle.construction.SingerWithInterface"
          p:name="John Butler"/>
</beans>